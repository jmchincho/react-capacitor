<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

  <!-- Si usas firma manual, indica el Team ID -->
  <key>teamID</key>
  <string>PV4A53A4T9</string>

  <!-- Estilo de firma manual -->
  <key>signingStyle</key>
  <string>manual</string>

  <key>method</key>
  <string>app-store-connect</string>

  <!-- Opciones de bitcode (comúnmente deshabilitadas en builds beta) -->
  <key>compileBitcode</key>
  <false/>
  <key>stripSwiftSymbols</key>
  <true/>
  <key>uploadBitcode</key>
  <false/>

  <key>signingCertificate</key>
  <string>Apple Distribution</string>

  <!-- Si requieres usar un provisioning profile específico, agrégalo así:
  <key>provisioningProfiles</key>
  <dict>
    <key>es.test.jmchincho</key>
    <string>es.test.jmchincho</string>
  </dict>
  -->
</dict>
</plist>
